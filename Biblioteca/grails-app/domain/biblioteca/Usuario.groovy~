package biblioteca

class Usuario {
	
	String nombre
	String aPaterno
	String aMaterno
	String direccion
		

    static constraints = {
		nombre(nullable:false)
		aPaterno(nullable:false)
		aMaterno(nullable:false)
		direccion(nullable:false)
    }
		
	String toString(){
		return nombre+" "+aPaterno+" "+aMaterno
	}

}
